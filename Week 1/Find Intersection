package week1.day2;
//Find Intersection 
public class Assignment1a {	
	public static void main(String[] args) 
	{
		int a[]={3,2,11,4,6,7}; //initialize array
		int b[]={1,2,8,4,9,7};
		for(int i =0 ;i<a.length;i++)
		{		
			for (int j = 0; j <b.length; j++)
			{		
				if(a[i]==b[j])
				{					
					System.out.println("The no "+ a[i] +" is present in both the Array a and b");
					break;
				}
		}
	}
}

}
